{
  "name": "project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "0.0.3",
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.11.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-socket.io": "^6.11.11",
    "@nestjs/schedule": "^0.2.1",
    "@nestjs/serve-static": "^1.0.2",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjs/websockets": "^6.11.11",
    "@types/bull": "^3.12.3",
    "@types/xlsx": "0.0.36",
    "bull": "^3.13.0",
    "memfs": "^3.1.2",
    "nest-router": "^1.0.9",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "typeorm-seeding": "^1.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.2",
    "@types/html-pdf": "^2.2.0",
    "@types/jest": "25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cpx": "^1.5.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.2",
    "html-pdf": "^2.2.0",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "mysql": "^2.17.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/identity/(.*)": "<rootDir>/libs/identity/src/$1",
      "@app/identity": "<rootDir>/libs/identity/src",
      "@app/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@app/core": "<rootDir>/libs/core/src",
      "@app/else/(.*)": "<rootDir>/libs/else/src/$1",
      "@app/else": "<rootDir>/libs/else/src",
      "@app/cms/(.*)": "<rootDir>/libs/cms/src/$1",
      "@app/cms": "<rootDir>/libs/cms/src"
    }
  }
}